{
  "name": "@ecomplus/utils",
  "version": "1.2.8",
  "description": "JS utility functions to E-Com Plus (not only) related apps",
  "main": "dist/ecom-utils.node.min.js",
  "module": "src/index.js",
  "browser": "dist/ecom-utils.min.js",
  "jsdelivr": "dist/ecom-utils.polyfill.min.js",
  "unpkg": "dist/ecom-utils.polyfill.min.js",
  "sideEffects": false,
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "node ./build/bin.js",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; npm ddp && standard-version && node ./build/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/ecomplus-utils.git"
  },
  "keywords": [
    "ecomplus",
    "ecommerce-apps",
    "ecommerce",
    "utility-functions",
    "storefront",
    "javascript-utils"
  ],
  "author": "E-Com Club <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/ecomplus-utils/issues"
  },
  "homepage": "https://github.com/ecomclub/ecomplus-utils#readme",
  "optionalDependencies": {
    "core-js": "3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.1",
    "docdash": "^1.1.1",
    "husky": "^4.0.0",
    "jsdoc": "^3.6.3",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ]
}
