{
  "name": "@ecomplus/utils",
  "version": "1.0.0",
  "description": "JS utility functions to E-Com Plus (not only) related apps",
  "main": "dist/ecom-utils.min.js",
  "module": "src/index.js",
  "browser": "dist/ecom-utils.min.js",
  "jsdelivr": "dist/ecom-utils.polyfill.min.js",
  "unpkg": "dist/ecom-utils.polyfill.min.js",
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "node ./build/bin.js",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; node ./build/bin.js & standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/ecomplus-utils.git"
  },
  "keywords": [
    "ecomplus",
    "ecommerce-apps",
    "ecommerce",
    "utility-functions",
    "storefront",
    "javascript-utils"
  ],
  "author": "E-Com Club <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/ecomplus-utils/issues"
  },
  "homepage": "https://github.com/ecomclub/ecomplus-utils#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "core-js": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "docdash": "^1.1.1",
    "husky": "^2.7.0",
    "jsdoc": "^3.6.3",
    "standard": "^12.0.1",
    "standard-version": "^6.0.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ]
}
